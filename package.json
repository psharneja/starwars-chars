{
  "name": "starwars-chars",
  "version": "0.0.0-development",
  "description": "Get random starwars-names",
  "main": "main/index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir dist --ignore *.spec.js src",
    "prebuild": "rimraf dist",
    "report-coverage": "cat ./coverage/lcov.info |  codecov",
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "cover": "nyc npm t",
    "watch:test": "npm t -- -w",
    "test": "mocha src/index.spec.js --compilers js:babel-register",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psharneja/starwars-chars.git"
  },
  "keywords": [
    "random",
    "star",
    "wars",
    "names"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Prabhsimran Singh <psharneja@gmail.com> (http://psharneja.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psharneja/starwars-chars/issues"
  },
  "homepage": "https://github.com/psharneja/starwars-chars#readme",
  "dependencies": {
    "unique-random-array": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.4",
    "mocha": "5.2.0",
    "nyc": "13.0.1",
    "rimraf": "2.6.2",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "5.0.9"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
