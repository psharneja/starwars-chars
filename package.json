{
  "name": "starwars-chars",
  "version": "0.0.0-development",
  "description": "Get random starwars-names",
  "main": "src/index.js",
  "scripts": {
    "report-coverage": "cat ./coverage/lcov.info |  codecov",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "test": "mocha src/index.spec.js -w",
    "test:single": "istanbul cover -x *.spec.js _mocha -- -R spec src/index.spec.js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psharneja/starwars-chars.git"
  },
  "keywords": [
    "random",
    "star",
    "wars",
    "names"
  ],
  "author": "Prabhsimran Singh <psharneja@gmail.com> (http://psharneja.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psharneja/starwars-chars/issues"
  },
  "homepage": "https://github.com/psharneja/starwars-chars#readme",
  "dependencies": {
    "unique-random-array": "1.0.1"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "5.0.9"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
